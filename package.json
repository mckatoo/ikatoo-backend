{
  "name": "ikatoo-backend",
  "version": "1.0.0",
  "description": "Back-end for ikatoo.com.br",
  "main": "index.js",
  "repository": "git@github.com:mckatoo/boilerplate-node.git",
  "author": "Milton Carlos Katoo <mckatoo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "lint": "yarn eslint",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "dev": "tsnd -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "prisma:generate": "prisma generate",
    "prisma:pull": "prisma db pull",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.0",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.16.7",
    "@prisma/client": "3.14.0",
    "@types/node": "^17.0.35",
    "@types/supertest": "2.0.12",
    "babel-plugin-module-resolver": "^4.1.0",
    "bcrypt": "5.0.1",
    "express": "4.18.1",
    "prisma": "3.14.0",
    "supertest": "6.2.3"
  }
}
