{
  "name": "ikatoo-backend",
  "version": "1.0.0",
  "description": "Back-end for ikatoo.com.br",
  "main": "index.js",
  "repository": "git@github.com:mckatoo/ikatoo-backend.git",
  "author": "Milton Carlos Katoo <mckatoo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "rm -rf dist && yarn build && node -r tsconfig-paths/register dist/src/index.js",
    "build": "tsc",
    "lint": "eslint --fix --ext .ts ./src",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "dev": "tsnd -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "test": "jest --clearCache && jest --maxWorkers=50%",
    "test:watch": "jest --clearCache && jest --watch --maxWorkers=25%",
    "prisma": "prisma --schema ./src/infra/database/prisma/schema.prisma",
    "prisma:generate": "yarn prisma generate",
    "prisma:pull": "yarn prisma db pull",
    "prisma:push": "yarn prisma db push",
    "prisma:migrate": "yarn prisma migrate dev",
    "prisma:migrate:reset": "yarn prisma migrate reset",
    "prisma:studio": "yarn prisma studio"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.5.1",
    "@types/node": "17.0.36",
    "@types/sqlite3": "3.1.8",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "eslint": "8.0.1",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "26.4.6",
    "eslint-plugin-n": "15.0.0",
    "eslint-plugin-promise": "6.0.0",
    "jest": "28.1.0",
    "supertest": "6.2.3",
    "ts-jest": "28.0.3",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.2"
  },
  "dependencies": {
    "@prisma/client": "3.14.0",
    "bcrypt": "5.0.1",
    "express": "4.18.1",
    "prisma": "3.14.0",
    "remove": "0.1.5"
  }
}
